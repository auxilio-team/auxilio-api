{
  "name": "Feedback",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": false
  },
  "mixins": {
    "TimeStamp": true,
    "SoftDelete": true,
    "RowCount": true
  },
  "properties": {
    "body": {
      "type": "string",
      "required": true
    },
    "files": {
      "type": ["object"]
    }
  },
  "validations": [],
  "relations": {
    "replays": {
      "type": "hasMany",
      "model": "FeedbackReplay"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "*"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "*"
    }
  ],
  "methods": {}
}
